// dustlib_k top-level forge.

forge DustlibK {

    const ABI_REVISION: UInt32 = 3;

    proc K::init() -> UInt32 {
        emit "dustlib_k initialized";
        return ABI_REVISION;
    }

    proc K::self_test() -> UInt32 {
        let mem = alloc(64);
        if mem == 0 {
            return 1;
        } else {
            free(mem);
            return 0;
        }
    }
}
